{
  "name": "@kitpo/tartarus",
  "version": "0.3.0",
  "description": "Tartarus package is used for our web store backend.",
  "license": "BSD-2-Clause",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc --build",
    "watch": "tsc-watch --esModuleInterop src/index.ts --outDir ./dist --onSuccess 'node ./dist/index.js'",
    "test-tartarus": "scripty",
    "test": "yarn test-tartarus",
    "lint": "scripty",
    "lint-fix": "scripty",
    "clean": "rimraf dist *.tsbuildinfo",
    "prettier": "scripty",
    "prettier-fix": "yarn prettier --config ../../.prettierrc --write 'src/**/*.ts'"
  },
  "dependencies": {
    "chalk": "^4.1.1",
    "cors": "^2.8.5",
    "db-migrate": "^0.11.12",
    "db-migrate-pg": "^1.2.2",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "pg": "^8.6.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.12",
    "@types/jasmine": "^3.7.6",
    "@types/pg": "^8.6.0",
    "@types/uuid": "^8.3.0",
    "jasmine": "^3.7.0",
    "jasmine-spec-reporter": "^7.0.0",
    "jasmine-ts": "^0.3.3",
    "ts-node": "^10.0.0",
    "tsc-watch": "^4.4.0",
    "typescript": "^4.3.2"
  },
  "scripty": {
    "path": "../../scripts/apps"
  }
}
